<!DOCTYPE html>
<html lang="de">
  <head>
    <meta charset="UTF-8" />
    <title>Schiedsgerichtstatus</title>
    <link rel="stylesheet" href="/assets/main.css"/>
  </head>
  <body>
    <div id='topnav'>
      <div id="header">
        <a href="/"><img id="logo" src="/assets/logo.png"/></a> Status der Schiedsgerichte
      </div>
      <div id="linking" align="right">
        <a href="/login">Login</a>&nbsp;
      </div>
    </div>
    <div id='content'>
      <table style="width: 100%;">
        <tr>
          <th>Schiedsgericht</th>
          <th>Rückmeldung</th>
          <th>Besetzung</th>
          <th>Geschäftslast</th>
        </tr>
      @Each.Courts
        <tr>
          <td>@Current.Name</td>
          <td>@Current.UpdateStatus</td>
          <td>@Current.JudgeStatus</td>
          <td>@Current.LoadStatus</td>
        </tr>
      @EndEach
      </table>
    </div>
  </body>
</html>